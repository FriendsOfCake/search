{
    "name": "friendsofcake/search",
    "description": "CakePHP Search plugin using PRG pattern",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "cakephp3",
        "cake3",
        "search"
    ],
    "homepage": "https://github.com/FriendsOfCake/search",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Winther",
            "role": "Author",
            "homepage": "http://cakephp.nu/"
        },
        {
            "name":"ADmad",
            "role":"Contributor",
            "homepage":"https://github.com/admad"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "friendsofcake/cakephp-test-utilities": "dev-master",
        "phpunit/phpunit": "<6.0",
        "cakephp/cakephp-codesniffer": "^3.0",
        "muffin/webservice": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Search\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Search\\Test\\": "tests",
            "Cake\\Test\\Fixture\\": "./vendor/cakephp/cakephp/tests/Fixture"
        }
    },
    "support": {
        "source": "https://github.com/FriendsOfCake/search",
        "issues": "https://github.com/FriendsOfCake/search/issues",
        "irc": "irc://irc.freenode.org/friendsofcake"
    },
    "scripts": {
        "cs-check": "vendor/bin/phpcs -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests"
    }
}
